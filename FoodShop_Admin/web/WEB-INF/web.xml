<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>listPAdmin</servlet-name>
        <servlet-class>controller.Product.listPAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>productUpdateServlet</servlet-name>
        <servlet-class>controller.Product.productUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayUpdate</servlet-name>
        <servlet-class>controller.Product.displayUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayOutStock</servlet-name>
        <servlet-class>controller.displayOutStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>toStock</servlet-name>
        <servlet-class>controller.toStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayListAccount</servlet-name>
        <servlet-class>controller.Account.displayListAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderTracking</servlet-name>
        <servlet-class>controller.Order.OrderTracking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeeMoreOrder</servlet-name>
        <servlet-class>controller.Order.SeeMoreOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>controller.dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcceptOrder</servlet-name>
        <servlet-class>controller.Order.AcceptOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>toOutStock</servlet-name>
        <servlet-class>controller.toOutStock</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>controller.login.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>controller.logout.logoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>listPAdmin</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>productUpdateServlet</servlet-name>
        <url-pattern>/editUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayUpdate</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayOutStock</servlet-name>
        <url-pattern>/outstock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>toStock</servlet-name>
        <url-pattern>/toStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayListAccount</servlet-name>
        <url-pattern>/account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderTracking</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeeMoreOrder</servlet-name>
        <url-pattern>/orderdetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AcceptOrder</servlet-name>
        <url-pattern>/orderaccept</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>toOutStock</servlet-name>
        <url-pattern>/toOutStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
